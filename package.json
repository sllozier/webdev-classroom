        "dotenv-webpack": "^8.0.1",
        "mini-css-extract-plugin": "^2.7.5",
        "style-loader": "^3.3.2",
        "webpack": "^5.79.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.7.3"
    }

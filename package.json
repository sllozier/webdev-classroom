{
    "name": "webdev_classroom",
    "version": "0.0.1",
    "description": "Web dev learning platform",
    "main": "webpack.config.js",
    "scripts": {
        "build": "webpack",
        "build:dev": "npm run build -- --watch --mode=development",
        "seed": "node server/db/seed",
        "start": "node server",
        "start:dev": "npm run build:dev & npm run start-server & npm run css-watch",
        "start:dev:logger": "LOGGING=true npm run start:dev",
        "start:dev:seed": "SEED=true npm run start:dev",
        "build:watch": "webpack -w",
        "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
        "test:dev": "npm run test -- --watch",
        "css-build": "webpack --mode production",
        "css-watch": "npm run css-build -- --watch"
    },
    "keywords": [
        "webdev",
        "fullstack",
        "classroom",
        "webpack",
        "noBulma"
    ],
    "author": "sllozier",
    "license": "MIT",
    "homepage": "https://github.com/sllozier/webdev-classroom",
    "repository": {
        "type": "git",
        "url": "https://github.com/sllozier/webdev-classroom.git"
    },
    "bugs": "https://github.com/sllozier/webdev-classroom/issues",
    "dependencies": {
        "@material-ui/core": "^4.12.4",
        "@material-ui/icons": "^4.11.3",
        "@react-oauth/google": "^0.12.1",
        "@reduxjs/toolkit": "^1.8.6",
        "axios": "^1.6.7",
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "hamburger-react": "^2.5.0",
        "history": "^5.3.0",
        "jsonwebtoken": "^9.0.2",
        "jwt-decode": "^4.0.0",
        "mocha": "^10.0.0",
        "moment": "^2.30.1",
        "morgan": "^1.9.0",
        "nodemon": "^2.0.16",
        "path": "^0.12.7",
        "pg": "^8.7.3",
        "react": "^18.1.0",
        "react-burger-menu": "^3.0.9",
        "react-dom": "^18.1.0",
        "react-google-login": "^5.2.2",
        "react-redux": "^v8.0.1",
        "react-router-dom": "^6.22.0",
        "recoil": "^0.7.7",
        "redux-logger": "^3.0.6",
        "redux-persist": "^6.0.0",
